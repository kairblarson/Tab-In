{
    "name": "Tab-In",
    "version": "1.0.0",
    "icons": {
        "16": "images/favicon.ico",
        "48": "images/favicon.ico",
        "128": "images/favicon.ico"
    },
    "description": "Tab-In is a great tool used for people like me that have a lot of tabs open, it allows you to easily search for any tab you want.",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "author": "Kai Larson",
    "action": {
        "default_popup": "index.html",
        "default_title": "Tab-In"
    },
    "permissions": ["tabs"],
    "commands": {
        "toggle_popup": {
            "suggested_key": {
                "default": "Ctrl+Shift+V",
                "mac": "Command+Shift+V"
            },
            "description": "Toggle feature foo"
        }
    }
}
